{
  "manifest_version": 2,
 
  "name": "Move Tab",
  "description": "This extension will move tab",
  "version": "1.0",
 
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "chrome://favicon/",
    "tabs",
    "bookmarks"
  ],

  "options_page": "options.html",

  "background": {
    "scripts": ["chrome-extension-async.js", "bg.js"],
    "persistent": false
  }

}